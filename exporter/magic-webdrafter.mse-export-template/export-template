script:
	write_card := {
		"\n"+card.name
		+"\n"+card.rarity
		+"\n"+card.card_color
		+"\n"+card.type
		+"\n"+cmc(card.casting_cost)
		+"\n"
		}
	write_cards := to_text(for each card in sort_list(cards, order_by: {input.card_number}) do write_card())
	to_string(
		 set.title+"\n"
		 +write_cards)
